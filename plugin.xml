		<titre>Agenda interne</titre>
	</bouton>
	<bouton id='messagerie' parent="outils_collaboratifs">
		<icone>images/message-16.png</icone>
		<titre>info_messagerie_interne</titre>
	</bouton>

	<pipeline>
		<nom>autoriser</nom>
		<inclure>organiseur_autoriser.php</inclure>
	</pipeline>

	<pipeline>
		<nom>declarer_tables_principales</nom>
		<inclure>base/organiseur.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_interfaces</nom>
		<inclure>base/organiseur.php</inclure>
	</pipeline>

	<pipeline>
