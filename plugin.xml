		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>

	<pipeline>
		<nom>configurer_liste_metas</nom>
		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>header_prive</nom>
		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>affiche_milieu</nom>
		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>affiche_droite</nom>
		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>affiche_auteurs_interventions</nom>
		<inclure>organiseur_pipelines.php</inclure>
	</pipeline>

	<pipeline><nom>agenda_rendu_evenement</nom><action></action></pipeline>
	<necessite id="grenier" version="[;]" />
</plugin>
